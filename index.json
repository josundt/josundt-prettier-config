{
  "$schema": "http://json.schemastore.org/prettierrc",
  "arrowParens": "avoid",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "strict",
  "plugins": [],
  "printWidth": 80,
  "proseWrap": "preserve",
  "quoteProps": "consistent",
  "semi": true,
  "singleQuote": false,
  "singleAttributePerLine": true,
  "trailingComma": "none",
  "overrides": [
    {
      "files": ["*.ejs"],
      "options": {
        "parser": "html"
      }
    }
  ]
}
